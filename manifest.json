{
    "manifest_version": 3,
    "name": "Canvas Obsidian Sync",
    "version": "1.0",
    "description": "Sync Canvas assignments to Obsidian todos.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
